{
  "name": "vue-typewriter",
  "description": "Vue3 Component for typewriter effects.",
  "author": "Jay Hsueh <xxj95719@gmail.com>",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/vue-typewriter.umd.js",
  "module": "dist/vue-typewriter.es.js",
  "types": "dist/types/install.d.ts",
  "exports": {
    ".": {
      "types": "./dist/install.d.ts",
      "import": "./dist/vue-typewriter.es.js",
      "require": "./dist/vue-typewriter.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.31.0",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0"
  }
}
