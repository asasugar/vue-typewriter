    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "terser": "^5.37.0",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vue-tsc": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asasugar/vue-typewriter.git"
  },
  "bugs": {
    "url": "https://github.com/asasugar/vue-typewriter/issues"
  },
  "homepage": "https://github.com/asasugar/vue-typewriter#readme",
  "keywords": [
    "vue",
